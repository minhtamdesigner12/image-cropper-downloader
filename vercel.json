{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "backend/download.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/download",
      "dest": "backend/download.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}
